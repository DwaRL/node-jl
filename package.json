{
	"name": "jl",
	"version": "2.1.2",
	"description": "Command-line JSON manipulation library",
	"keywords": [
		"json",
		"sql",
		"cli",
		"commandline",
		"pipe"
	],
	"homepage": "https://github.com/avz/node-jl",
	"repository": {
		"type" : "git",
		"url" : "https://github.com/avz/node-jl.git"
	},
	"license": "BSD-2-Clause",
	"author": "Artem Zaytsev <arepo@nologin.ru>",
	"main": "./main.js",

	"bin": {
		"jl-count": "bin/jl-count",
		"jl-extract": "bin/jl-extract",
		"jl-filter": "bin/jl-filter",
		"jl-from-csv": "bin/jl-from-csv",
		"jl-plainify": "bin/jl-plainify",
		"jl-reduce": "bin/jl-reduce",
		"jl-sort": "bin/jl-sort",
		"jl-sql": "bin/jl-sql",
		"jl-sum": "bin/jl-sum",
		"jl-transform": "bin/jl-transform"
	},

	"engines": {
		"node": ">=0.10"
	},

	"dependencies": {
